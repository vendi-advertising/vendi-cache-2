{
    "name": "vendi-advertising/vendi-cache",
    "type": "library",
    "description" : "Vendi Cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Haas",
            "email": "chris@vendiadvertising.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "WordPress/wordpress-develop",
                "version": "master",
                "source": {
                    "url": "https://github.com/WordPress/wordpress-develop.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "webmozart/path-util": "^2.3",
        "league/flysystem": "^1.0",
        "monolog/monolog": "^1.0",
        "ramsey/uuid": "^3.7",
        "symfony/http-foundation": "^2 || ^3",
        "beberlei/assert": "^2.7",
        "naneau/semver": "^0.0.7",
        "sgh/comparable": "^1.1",
        "psr/log": "^1.0",
        "wazaari/monolog-mysql": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^2 || ^3",
        "phpunit/phpunit": "*",
        "wordpress/wordpress-develop": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "symfony/process": "^2 || ^3",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "jakub-onderka/php-console-highlighter": "^0.3",
        "phpro/grumphp": "*",
        "wp-cli/wp-cli": "^1.3"
    },
    "config": {
        "optimize-autoloader": true
    }
}
