{
    "name": "vendi-advertising/vendi-cache",
    "type": "library",
    "description" : "Vendi Cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Haas",
            "email": "chris@vendiadvertising.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "WordPress/wordpress-develop",
                "version": "master",
                "source": {
                    "url": "https://github.com/WordPress/wordpress-develop.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "webmozart/path-util": "^2.3",
        "league/flysystem": "^1.0",
        "vendi-advertising/vendi-shared-utils": "^4.0",
        "monolog/monolog": "^1.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "phpunit/phpunit": "^6.3",
        "wordpress/wordpress-develop": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "symfony/process": "^3.3"
    },
    "config": {
        "optimize-autoloader": true
    }
}
